<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.maoxian.mapper.RequestChainMapper">
    <select id="selectList" resultType="RequestChain">
        select id, time, status, waf_id, parent_id, request_id
        from request_chain
        where request_id = #{requestId}
    </select>
</mapper>