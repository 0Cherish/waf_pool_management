<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.maoxian.mapper.WafMapper">

    <select id="queryWafById" resultType="Waf">
        select id, name, ip, port, version, status, description from waf where id = #{id}
    </select>
</mapper>